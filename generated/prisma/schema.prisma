// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int      @id @unique @default(autoincrement())
  title       String
  link        String   @unique
  description String
  preview     String
  images      String[]
  subBody     String
  subBodyJSON String
  body        String
  bodyJSON    String
  table       String?
  tableJSON   String?
}
